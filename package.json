{
  "name": "szmaciura-racer-v2",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:backend": "turbo run build --filter=backend",
    "build:web": "turbo run build --filter=web",
    "dev": "turbo run dev",
    "dev:backend": "turbo run dev --filter=backend",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "2.8.7",
    "turbo": "1.8.8",
    "typescript": "5.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "nanoid": "3.3.6",
    "zod": "3.21.4",
    "lodash": "4.17.21",
    "date-fns": "2.29.3"
  },
  "packageManager": "pnpm@8.5.0"
}
