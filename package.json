{
  "name": "szmaciura-racer-v2",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:web": "turbo run build --filter=web",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "1.8.8",
    "typescript": "5.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "nanoid": "3.3.6"
  },
  "packageManager": "pnpm@7.28.0"
}
